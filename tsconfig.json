{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"paths": {
			"@components/*": [
				"./src/components/*"
			],
			"@models/*": [
				"./src/types/*"
			],
			"@services/*": [
				"./src/services/*"
			],
			"@types/*": [
				"./src/types/*"
			],
			"$lib/*": [
				"./src/lib/*"
			]
		}
	}
}
